{
  "ConnectionStrings": {
    "Postgres": "Server=postgres10;Port=5432;Database=MonitorDB;User Id=monitor;Password=monitor",
    "SqlServer": "Server=sql2017linux;Database=MonitorDB;uid=cms;pwd=cms;Trusted_Connection=false;MultipleActiveResultSets=true"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "DefaultUrl": {
    "Url": "{controller=Admin}/{action=WelcomePage}/{id?}"
  },
  "UseConnection": {
    "DefaultConnection": "Postgres"
  },
  "ConfigSystem": {
    "RequireDigit": "true",
    "RequireNonAlphanumeric": "true",
    "RequiredLength": "6",
    "RequireLowercase": "false",
    "RequireUppercase": "false",
    "AllowedForNewUsers": "true",
    "MaxFailedAccessAttempts": "5",
    "DefaultLockoutTimeSpanDays": "4000",
    "CookieExpireTimeSpanMin": "30",
    "LoginPath": "/login",
    "LogoutPath": "/logout",
    "AccessDeniedPath": "/Admin/AccessDenied"
  },
  "WelcomePage": {
    "Message": "This is the MonitorDB"
  }

}
